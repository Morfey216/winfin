section.multi-examples.container
  //- div.container
  +title('Наши проекты деревянных&nbsp;окон', 'section-nodecor').multi-examples__title
  div.multi-examples__controls
    +button('Для домов', 'control').active.js-mec-control-button(data-mec-control='set-1' type='button')
    +button('Для офисов', 'control').js-mec-control-button(data-mec-control='set-2' type='button')
    +button('Для дачи', 'control').js-mec-control-button(data-mec-control='set-3' type='button')
    +button('Для квартир', 'control').js-mec-control-button(data-mec-control='set-4' type='button')

  div.multi-examples__contents
    div.multi-examples__set(data-mec-control='set-1')
      div.multi-examples__container.multi-examples__gallery-top
        div.multi-examples__wrapper
          each ex in examplesList
            img(src="img/examples/" + ex.image width=ex.w height=ex.h alt=ex.alt).multi-examples__slide
      div.multi-examples__container.multi-examples__gallery-thumbs
        div.multi-examples__thumbs-wrapper
          each thumbs in thumbsList
            img(src="img/examples/" + thumbs.image width=thumbs.w height=thumbs.h alt=thumbs.alt).multi-examples__thumbs-slide
    div.multi-examples__set(data-mec-control='set-2')
